#set ($template = $data.getTemplateInfo())
$!template.setLayoutTemplate("/ScreenOnly.vm")

<div>The DICOM header values for the first dicom file in this scan.</div>
<table>
	<tr>
		<th>Tag</th>
		<th>Desc</th>
		<th>Value</th>
	</tr>
#foreach($row in $table.toArrayListOfLists())
	<tr>
		<td>$!row.get(0)</td>
		<td>$!row.get(4)</td>
		<td>$!row.get(3)</td>
	</tr>
#end
</table>